products.sort((a,b)=>{
    let pa = a.name.toLowerCase() ,
        pb = b.name.toLowerCase()
    if(pa > pb){
        return -1;
    }
    else  if(pa < pb){
        return 1;
    }
    else{
        return 0 ;
    }
});
